	<xs:complexType name="gyrokinetics_model">
		<xs:annotation>
			<xs:documentation>Description of the GK model assumptions</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="include_a_field_parallel">
				<xs:annotation>
					<xs:documentation>Flag = 1 if fluctuations of the parallel vector potential are retained, 0 otherwise</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="include_b_field_parallel">
				<xs:annotation>
					<xs:documentation>Flag = 1 if fluctuations of the parallel magnetic field are retained, 0 otherwise</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="use_mhd_approximation">
				<xs:annotation>
					<xs:documentation>Flag = 1 if the geometric dependence of the grad-B drift is approximated to be that of the curvature drift (including the pressure gradient contribution), 0 otherwise. Using this approximation (Flag=1) is only recommended together with the neglect of parallel magnetic field fluctuations</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
						<introduced_after_version>3.41.0</introduced_after_version>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="include_coriolis_drift">
				<xs:annotation>
					<xs:documentation>Flag = 1 if Coriolis drift is included, 0 otherwise</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="include_centrifugal_effects">
				<xs:annotation>
					<xs:documentation>Flag = 1 if centrifugal effects are retained, 0 otherwise</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="collisions_pitch_only">
				<xs:annotation>
					<xs:documentation>Flag = 1 if only pitch-angle scattering is retained, 0 otherwise</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="collisions_momentum_conservation">
				<xs:annotation>
					<xs:documentation>Flag = 1 if the collision operator conserves momentum, 0 otherwise</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="collisions_energy_conservation">
				<xs:annotation>
					<xs:documentation>Flag = 1 if the collision operator conserves energy, 0 otherwise</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="collisions_finite_larmor_radius">
				<xs:annotation>
					<xs:documentation>Flag = 1 if finite larmor radius effects are retained in the collision operator, 0 otherwise</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="adiabatic_electrons">
				<xs:annotation>
					<xs:documentation>Flag = 1 if electrons are adiabatic, 0 otherwise</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
