						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="real0d" type="numerics_solver_1d_equation_control_float" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Set of real type scalar control parameters</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
