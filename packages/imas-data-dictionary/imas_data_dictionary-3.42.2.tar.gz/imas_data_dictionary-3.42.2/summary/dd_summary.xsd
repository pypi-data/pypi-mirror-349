	<xs:complexType name="summary_disruption_decay_times">
		<xs:annotation>
			<xs:documentation>Disruption decay times for one quantity</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="linear_20_80" type="summary_constant_flt_0d">
				<xs:annotation>
					<xs:documentation>Decay time defined as (t(0.2)-t(0.8))/0.6, where t(X) corresponds to the time where this quantity reaches X*100% of its pre-disruptive value</xs:documentation>
					<xs:appinfo>
						<units>s</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="linear_custom" type="summary_disruption_decay_linear">
				<xs:annotation>
					<xs:documentation>Decay time defined as (t(X2)-t(X1))/(X1-X2), where t(Xj) corresponds to the time where this quantity reaches Xj*100% of its pre-disruptive value </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="exponential" type="summary_constant_flt_0d">
				<xs:annotation>
					<xs:documentation>Exponential decay time (tau) used when the process is described by an exponential function (exp(-(t-t0)/tau))). Here "t" is time and "t0" is the time where the decay process starts</xs:documentation>
					<xs:appinfo>
						<units>s</units>
					</xs:appinfo>
				</xs:annotation>
