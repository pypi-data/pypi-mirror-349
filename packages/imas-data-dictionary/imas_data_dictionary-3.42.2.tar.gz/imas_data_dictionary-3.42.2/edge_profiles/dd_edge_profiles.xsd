			<xs:element name="j_heat_viscosity" type="generic_grid_vector_components" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Current density due to the heat viscosity, given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<units>A.m^-2</units>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="j_pfirsch_schlueter" type="generic_grid_vector_components" maxOccurs="unbounded">
