			</xs:element>
			<xs:element name="j_parallel_viscosity" type="generic_grid_vector_components" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Current density due to the parallel viscosity, given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<units>A.m^-2</units>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="j_perpendicular_viscosity" type="generic_grid_vector_components" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Current density due to the perpendicular viscosity, given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<units>A.m^-2</units>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
