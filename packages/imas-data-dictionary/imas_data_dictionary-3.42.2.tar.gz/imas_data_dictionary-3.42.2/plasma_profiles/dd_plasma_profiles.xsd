			<xs:element name="temperature" type="generic_grid_scalar_single_position" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Temperature, given at various positions (grid subset of size 1)</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
						<units>eV</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="density" type="generic_grid_scalar_single_position" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Density (thermal+non-thermal), given at various positions (grid subset of size 1)</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
						<units>m^-3</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="plasma_profiles_time_slice">
		<xs:annotation>
			<xs:documentation>Plasma GGD description for a given time slice</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="electrons" type="plasma_profiles_time_slice_electrons">
				<xs:annotation>
					<xs:documentation>Quantities related to the electrons</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ion" type="plasma_profiles_time_slice_ion" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Quantities related to the different ion species</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="neutral" type="plasma_profiles_time_slice_neutral" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Quantities related to the different neutral species</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="t_i_average" type="generic_grid_scalar" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Ion temperature (averaged on ion species), given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
						<units>eV</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="n_i_total_over_n_e" type="generic_grid_scalar" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Ratio of total ion density (sum over ion species) over electron density. (thermal+non-thermal), given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="n_i_total" type="generic_grid_scalar" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Total ion density (sum over ion species and thermal+non-thermal), given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
						<units>m^-3</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="zeff" type="generic_grid_scalar" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Effective charge, given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="pressure_thermal" type="generic_grid_scalar" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Thermal pressure (electrons+ions), given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
						<units>Pa</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="pressure_perpendicular" type="generic_grid_scalar" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Total perpendicular pressure (electrons+ions, thermal+non-thermal), given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
						<units>Pa</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="pressure_parallel" type="generic_grid_scalar" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Total parallel pressure (electrons+ions, thermal+non-thermal), given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
						<units>Pa</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="j_total" type="generic_grid_vector_components" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Total current density, given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<units>A.m^-2</units>
						<coordinate1>1...N</coordinate1>
						<introduced_after_version>3.32.1</introduced_after_version>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="r_j_total_phi" type="generic_grid_scalar" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Toroidal component of the current density multiplied by the local major radius, given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
						<units>A.m^-1</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="j_parallel" type="generic_grid_scalar" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Current due to parallel electric and thermo-electric conductivity and potential and electron temperature gradients along the field line, differences away from ambipolar flow in the parallel direction between ions and electrons (this is not the parallel component of j_total)</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
						<units>A.m^-2</units>
						<introduced_after_version>3.32.1</introduced_after_version>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="j_anomalous" type="generic_grid_vector_components" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Anomalous current density, given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<units>A.m^-2</units>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="j_inertial" type="generic_grid_vector_components" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Inertial current density, given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<units>A.m^-2</units>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="j_ion_neutral_friction" type="generic_grid_vector_components" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Current density due to ion neutral friction, given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<units>A.m^-2</units>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="j_parallel_viscosity" type="generic_grid_vector_components" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Current density due to the parallel viscosity, given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<units>A.m^-2</units>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="j_perpendicular_viscosity" type="generic_grid_vector_components" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Current density due to the perpendicular viscosity, given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<units>A.m^-2</units>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="j_heat_viscosity" type="generic_grid_vector_components" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Current density due to the heat viscosity, given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<units>A.m^-2</units>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="j_pfirsch_schlueter" type="generic_grid_vector_components" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Current density due to Pfirsch-Schlter effects, given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<units>A.m^-2</units>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="j_diamagnetic" type="generic_grid_vector_components" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Current density due to the diamgnetic drift, given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<units>A.m^-2</units>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="e_field" type="generic_grid_vector_components" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Electric field, given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
						<units>V.m^-1</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="b_field" type="generic_grid_vector_components" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Magnetic field, given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
						<units>T</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="a_field" type="generic_grid_vector_components" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Magnetic vector potential, given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
						<units>T.m</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="phi_potential" type="generic_grid_scalar" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Electric potential, given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
						<units>V</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="psi" type="generic_grid_scalar" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Poloidal flux, given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
						<units>Wb</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="mass_density" type="generic_grid_scalar" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Mass density, given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
						<units>kg.m^-3</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="velocity_mass_centre" type="generic_grid_vector_components" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Velocity of the centre of mass of all the species (see equation 2 in the paper linked below), given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<units>m.s^-1</units>
						<coordinate1>1...N</coordinate1>
						<url>https://iopscience.iop.org/article/10.1088/1361-6587/ac414d</url>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="velocity_parallel_over_b_field" type="generic_grid_scalar" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Parallel (to magnetic field) velocity of the centre of mass of all the species (see equation 2 in the paper linked below), divided by the module of the local magnetic field, given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<units>m.s^-1.T^-1</units>
						<coordinate1>1...N</coordinate1>
						<url>https://iopscience.iop.org/article/10.1088/1361-6587/ac414d</url>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="vorticity" type="generic_grid_vector_components" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Curl of the velocity_mass_centre vector, given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<units>s^-1</units>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="vorticity_over_r" type="generic_grid_vector_components" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Vorticity divided by the local major radius, given on various grid subsets</xs:documentation>
					<xs:appinfo>
						<units>m^-1.s^-1</units>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="time" type="flt_type">
				<xs:annotation>
					<xs:documentation>Time</xs:documentation>
					<xs:appinfo>
						<units>s</units>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="plasma_profiles_ggd_fast">
		<xs:annotation>
			<xs:documentation>Quantities provided at a faster sampling rate than the full ggd quantities, on a reduced set of positions. Positions are described by a set of grid_subsets of size 1</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="electrons" type="plasma_profiles_ggd_fast_electrons">
				<xs:annotation>
					<xs:documentation>Quantities related to the electrons</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ion" type="plasma_profiles_ggd_fast_ion" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Quantities related to the different ion species</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="energy_thermal" type="generic_grid_scalar_single_position" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Plasma energy content = 3/2 * integral over the volume of the grid subset of the thermal pressure (summed over all species), for various grid subsets</xs:documentation>
					<xs:appinfo>
						<units>J</units>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="time" type="flt_type">
				<xs:annotation>
					<xs:documentation>Time</xs:documentation>
					<xs:appinfo>
						<units>s</units>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="plasma_profiles">
		<xs:annotation>
			<xs:documentation>Plasma profiles (core and/or edge). This IDS is a prototype based on the merge of core_profiles, edge_profiles and the MHD IDS, for testing purposes only at this stage. It contains magnetic field maps to describe magnetic field fluctuations, while the equilibrium magnetic field is described in the equilibrium IDS</xs:documentation>
			<xs:appinfo>
				<lifecycle_status>alpha</lifecycle_status>
				<lifecycle_version>3.42.0</lifecycle_version>
				<lifecycle_last_change>3.42.0</lifecycle_last_change>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ids_properties"/>
				<xs:element name="vacuum_toroidal_field" type="b_tor_vacuum_1">
					<xs:annotation>
						<xs:documentation>Characteristics of the vacuum toroidal field (used in rho_tor definition and in the normalization of current densities)</xs:documentation>
						<xs:appinfo>
							<cocos_alias>IDSPATH</cocos_alias>
							<cocos_replace>plasma_profiles.vacuum_toroidal_field</cocos_replace>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="midplane" type="identifier_static">
					<xs:annotation>
						<xs:documentation>Choice of midplane definition (use the lowest index number if more than one value is relevant) </xs:documentation>
						<xs:appinfo>
							<doc_identifier>utilities/midplane_identifier.xml</doc_identifier>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="global_quantities" type="plasma_profiles_global_quantities">
					<xs:annotation>
						<xs:documentation>Various global quantities derived from the profiles</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="profiles_1d" type="plasma_profiles_profiles_1d" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Radial profiles for various time slices. Flux averaged profiles in the closed field line region (grid/rho_pol_norm &lt;= 1) and taking local values on outboard equatorial mid-plane when in the SOL (grid/rho_pol_norm &gt; 1)</xs:documentation>
						<xs:appinfo>
							<coordinate1>time</coordinate1>
							<type>dynamic</type>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="profiles_2d" type="plasma_profiles_profiles_2d" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Plasma quantities in a poloidal cross section, for various time slices</xs:documentation>
						<xs:appinfo>
							<coordinate1>time</coordinate1>
							<type>dynamic</type>
							<cocos_alias>IDSPATH</cocos_alias>
							<cocos_replace>core_profiles.profiles_2d{i}</cocos_replace>
							<introduced_after_version>3.38.1</introduced_after_version>
							<lifecycle_status>alpha</lifecycle_status>
							<lifecycle_version>3.39.0</lifecycle_version>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="grid_ggd" type="generic_grid_aos3_root" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Grid (using the Generic Grid Description), for various time slices. The timebase of this array of structure must be a subset of the ggd timebase</xs:documentation>
						<xs:appinfo>
							<coordinate1>time</coordinate1>
							<type>dynamic</type>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="ggd" type="plasma_profiles_time_slice" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Plasma quantities represented using the general grid description, for various time slices. The timebase of this array of structure must be a subset of the ggd_fast timebase (only if the ggd_fast array of structure is used)</xs:documentation>
						<xs:appinfo>
							<coordinate1>time</coordinate1>
							<type>dynamic</type>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="ggd_fast" type="plasma_profiles_ggd_fast" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Quantities provided at a faster sampling rate than the full ggd quantities. These are either integrated quantities or local quantities provided on a reduced set of positions. Positions and integration domains are described by a set of grid_subsets (of size 1 for a position). </xs:documentation>
						<xs:appinfo>
							<coordinate1>time</coordinate1>
							<type>dynamic</type>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="covariance" type="covariance_matrix">
					<xs:annotation>
						<xs:documentation>User defined covariance matrix. The covariance of various quantities can be stored here, these quantities are referred to by giving their IDS path in the rows_uri list</xs:documentation>
						<xs:appinfo>
							<introduced_after_version>3.39.0</introduced_after_version>
							<lifecycle_status>alpha</lifecycle_status>
							<lifecycle_version>3.40.0</lifecycle_version>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="statistics" type="statistics" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Statistics for various time slices</xs:documentation>
						<xs:appinfo>
							<coordinate1>time</coordinate1>
							<type>dynamic</type>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element ref="code"/>
				<xs:element ref="time"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
