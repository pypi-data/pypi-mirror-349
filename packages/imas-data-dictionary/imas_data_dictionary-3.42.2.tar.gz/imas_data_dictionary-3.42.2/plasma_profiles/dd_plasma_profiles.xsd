						<type>dynamic</type>
						<coordinate1>../../../grid/dim1</coordinate1>
						<coordinate2>../../../grid/dim2</coordinate2>
						<units>as_parent</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="parallel">
				<xs:annotation>
					<xs:documentation>Parallel component</xs:documentation>
