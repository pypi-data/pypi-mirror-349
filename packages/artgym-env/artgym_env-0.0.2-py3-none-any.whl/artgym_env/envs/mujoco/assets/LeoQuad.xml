    <sensor>
        <touch name="FLcontactSensor" site="FL_FOOT" />
        <touch name="BLcontactSensor" site="BL_FOOT" />
        <touch name="FRcontactSensor" site="FR_FOOT" />
        <touch name="BRcontactSensor" site="BR_FOOT" />
        <gyro name="torso-angular-velocity" site="IMU" noise="1e-3" cutoff="97" />
        <accelerometer name="torso-linear-acceleration" site="IMU" noise="1e-2" cutoff="97" />
        <!--
		Depth sensors (ray-casting)
		dim : dimension of sensor data
		datatype : data type(real(for scalar), positive, axis, quaternion) of each sensor data
		user : [user_sensor_type(0:1D,1:2D,2:Omni), fov_x(rad), fov_y(rad), dim_x(number of rays in x-direction), dist_min(m), dist_max(m), noise_mean, noise_std]
		axis(optical frame) :
		x : right
		y : down
		z : look-at direction
		-->
