    <actuator>
        <motor name="FL_Roll" gear="1" joint="FL_HR_JOINT" ctrllimited="true" ctrlrange="-3 3" />
        <motor name="FL_Pitch" gear="1" joint="FL_HP_JOINT" ctrllimited="true" ctrlrange="-3 3" />
        <motor name="FL_Knee" gear="1" joint="FL_KN_JOINT" ctrllimited="true" ctrlrange="-3 3" />
        <motor name="BL_Roll" gear="1" joint="BL_HR_JOINT" ctrllimited="true" ctrlrange="-3 3" />
        <motor name="BL_Pitch" gear="1" joint="BL_HP_JOINT" ctrllimited="true" ctrlrange="-3 3" />
        <motor name="BL_Knee" gear="1" joint="BL_KN_JOINT" ctrllimited="true" ctrlrange="-3 3" />
        <motor name="FR_Roll" gear="1" joint="FR_HR_JOINT" ctrllimited="true" ctrlrange="-3 3" />
        <motor name="FR_Pitch" gear="1" joint="FR_HP_JOINT" ctrllimited="true" ctrlrange="-3 3" />
        <motor name="FR_Knee" gear="1" joint="FR_KN_JOINT" ctrllimited="true" ctrlrange="-3 3" />
        <motor name="BR_Roll" gear="1" joint="BR_HR_JOINT" ctrllimited="true" ctrlrange="-3 3" />
        <motor name="BR_Pitch" gear="1" joint="BR_HP_JOINT" ctrllimited="true" ctrlrange="-3 3" />
        <motor name="BR_Knee" gear="1" joint="BR_KN_JOINT" ctrllimited="true" ctrlrange="-3 3" />
    </actuator>
    <contact>
        <exclude name="FL_1" body1="BODY" body2="FL_HP" />
        <exclude name="FL_2" body1="FL_HP" body2="FL_THIGH" />
        <exclude name="FL_3" body1="FL_THIGH" body2="FL_CALF" />
        <exclude name="FL_4" body1="BODY" body2="FL_THIGH" />
        <exclude name="FL_5" body1="BODY" body2="FL_CALF" />
        <exclude name="BL_1" body1="BODY" body2="BL_HP" />
        <exclude name="BL_2" body1="BL_HP" body2="BL_THIGH" />
        <exclude name="BL_3" body1="BL_THIGH" body2="BL_CALF" />
        <exclude name="BL_4" body1="BODY" body2="BL_THIGH" />
        <exclude name="BL_5" body1="BODY" body2="BL_CALF" />
        <exclude name="FR_1" body1="BODY" body2="FR_HP" />
        <exclude name="FR_2" body1="FR_HP" body2="FR_THIGH" />
        <exclude name="FR_3" body1="FR_THIGH" body2="FR_CALF" />
        <exclude name="FR_4" body1="BODY" body2="FR_THIGH" />
        <exclude name="FR_5" body1="BODY" body2="FR_CALF" />
        <exclude name="BR_1" body1="BODY" body2="BR_HP" />
        <exclude name="BR_2" body1="BR_HP" body2="BR_THIGH" />
        <exclude name="BR_3" body1="BR_THIGH" body2="BR_CALF" />
        <exclude name="BR_4" body1="BODY" body2="BR_THIGH" />
        <exclude name="BR_5" body1="BODY" body2="BR_CALF" />
