            <site name="LB" pos="-0.2911 0 -0.09975" axisangle="1 0 0 3.141592" type="cylinder" size="0.0015" />

            <body name="FL_HP" pos="0.2126 0.055 0">
                <inertial pos="0.068 0.009 0" quat="0.53355 0.53355 0.464031 0.464031" mass="1.271" diaginertia="0.00814005 0.008 0.000859945" />
                <joint name="FL_HR_JOINT" pos="0 0 0" axis="1 0 0" range="-3.14 3.14" type="hinge"/>
