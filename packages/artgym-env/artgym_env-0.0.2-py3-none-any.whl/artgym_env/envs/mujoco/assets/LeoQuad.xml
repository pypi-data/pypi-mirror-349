            </body>
            <body name="FR_HP" pos="0.2126 -0.055 0">
                <inertial pos="0.068 -0.009 0" quat="0.464031 0.464031 0.53355 0.53355" mass="1.271" diaginertia="0.00814005 0.008 0.000859945" />
                <joint name="FR_HR_JOINT" pos="0 0 0" axis="1 0 0" range="-3.14 3.14" type="hinge"/>
                <geom type="mesh" material="quadruped" mesh="FR_HP" group="1" />
                <body name="FR_THIGH" pos="0.0785 0 0">
                    <inertial pos="-0.001 -0.101 -0.03" quat="0.86289 0.505392 0 0" mass="2.442" diaginertia="0.044 0.0340475 0.0129525" />
                    <joint name="FR_HP_JOINT" pos="0 0 0" axis="0 1 0" range="-3.14 3.14" type="hinge"/>
