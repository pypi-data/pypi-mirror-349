                        <joint name="FR_KN_JOINT" pos="0 0 0" axis="0 1 0" range="-3.14 3.14" type="hinge"/>
                        <geom type="mesh" material="quadruped" mesh="FR_CALF" group="1" />
                        <site name="FR_FOOT" pos="0 0 -0.329" type="sphere" size="0.001" />
                    </body>
