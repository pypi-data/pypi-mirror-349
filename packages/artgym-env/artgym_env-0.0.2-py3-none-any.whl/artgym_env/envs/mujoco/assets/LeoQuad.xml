<mujoco model="LeoQuad">
    <compiler angle="radian" meshdir="meshes/" autolimits="true" />
    <option timestep="0.001" tolerance="1e-06" cone="elliptic" jacobian="sparse" solver="CG" iterations="30" />
    <default>
        <joint armature="1" damping="1" limited="true"/>
        <geom conaffinity="0" condim="3" density="5.0" friction="1 0.5 0.5" margin="0.01" rgba="0.8 0.6 0.4 1"/>
    </default>
    <asset>
        <texture builtin="gradient" height="100" rgb1="1 1 1" rgb2="0 0 0" type="skybox" width="100"/>
        <texture builtin="flat" height="1278" mark="cross" markrgb="1 1 1" name="texgeom" random="0.01" rgb1="0.8 0.6 0.4" rgb2="0.8 0.6 0.4" type="cube" width="127"/>
        <texture builtin="checker" height="100" name="texplane" rgb1="0 0 0" rgb2="0.8 0.8 0.8" type="2d" width="100"/>
        <material name="MatPlane" reflectance="0.5" shininess="1" specular="1" texrepeat="60 60" texture="texplane"/>
        <material name="geom" texture="texgeom" texuniform="true"/>
    </asset>
    <asset>
        <mesh name="BODY" file="BODY.STL" />
        <mesh name="FL_HP" file="FL_HP.STL" />
        <mesh name="FL_THIGH" file="FL_THIGH.STL" />
        <mesh name="FL_CALF" file="FL_CALF.STL" />
        <mesh name="BL_HP" file="BL_HP.STL" />
        <mesh name="BL_THIGH" file="BL_THIGH.STL" />
        <mesh name="BL_CALF" file="FL_CALF.STL" />
        <mesh name="FR_HP" file="FR_HP.STL" />
        <mesh name="FR_THIGH" file="FR_THIGH.STL" />
        <mesh name="FR_CALF" file="FR_CALF.STL" />
        <mesh name="BR_HP" file="BR_HP.STL" />
        <mesh name="BR_THIGH" file="BR_THIGH.STL" />
        <mesh name="BR_CALF" file="FR_CALF.STL" />
        <material name="quadruped" rgba="0.75294 0.75294 0.75294 1" />
    </asset>
    <worldbody>
        <light cutoff="100" diffuse="1 1 1" dir="-0 0 -1.3" directional="true" exponent="1" pos="0 0 1.3" specular=".1 .1 .1"/>
        <geom conaffinity="1" condim="3" material="MatPlane" name="floor" pos="0 0 0" rgba="0.8 0.9 0.8 1" size="40 40 40" type="plane"/>
        <body name="BODY" pos="0 0 0.75">
            <joint armature="0" damping="0" limited="false" margin="0.01" name="BODY" pos="0 0 0" type="free"/>
            <inertial pos="0.004 0.006 -0.005" quat="0.0428995 0.709987 0.0435274 0.701558" mass="11.359" diaginertia="0.301405 0.276631 0.0499641" />
            <site name="IMU" size="0.01" pos="0 0 0" />
            <geom type="mesh" material="quadruped" mesh="BODY" group="1" />

            <!-- Camera(RGB-D) frame
            x : right
            y : down
            z : look-at
            * this is identical with ROS optical frame

            angle axis for the front-lower(CF2) camera
            70deg : -0.7040626 0.7040626 -0.1226916 2.8567377
            75deg : -0.7040626 0.7040626 -0.0926916 2.9567377
            80deg : -0.7057576 0.7057576 -0.0617458 3.0182577
            85deg : -0.70677 0.70677 -0.0308582 3.0798957 (current setting)
            90deg : 0.7071068 -0.7071068 0 3.1415927
            95deg :  0.70677 -0.70677 -0.0308582 3.0798957
            100deg : 0.7057576 -0.7057576 -0.0617458 3.0182577
            105deg : 0.7040626 -0.7040626 -0.0926916 2.9567377
            //-->
            <site name="CF1" pos="0.379 0 0.029" axisangle="-0.5773503 0.5773503 -0.5773503 2.0943951" type="cylinder" size=".0015" />
